{
  "name": "tardigrades/sexy-field-doctrine",
  "description": "SexyField Doctrine package, containing config generator, writer, reader and deleter",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Dion Snoeijen",
      "email": "hallo@dionsnoeijen.nl"
    }
  ],
  "require": {
    "php": "^7.4",
    "doctrine/orm": "^2.5.6",
    "symfony/dependency-injection": "^4",
    "symfony/yaml": "^4",
    "symfony/config": "^4"
  },
  "autoload": {
    "psr-4": {
      "Tardigrades\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tardigrades\\": "test/unit/"
    }
  },
  "require-dev": {
    "tardigrades/sexy-field": "dev-integrate-create-and-delete-options",
    "doctrine/doctrine-bundle": "^1.6",
    "phpunit/phpunit": "^7.0",
    "mockery/mockery": "^1.0",
    "mikey179/vfsstream": "^1.6",
    "squizlabs/php_codesniffer": "^3.2"
  }
}
