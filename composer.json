{
  "name": "tardigrades/sexy-field-doctrine",
  "description": "SexyField Doctrine package, containing config generator, writer, reader and deleter",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Dion Snoeijen",
      "email": "hallo@dionsnoeijen.nl"
    }
  ],
  "require": {
    "php": "8.*",
    "doctrine/orm": "^2.8",
    "symfony/dependency-injection": "^5",
    "symfony/yaml": "^5",
    "symfony/config": "^5"
  },
  "autoload": {
    "psr-4": {
      "Tardigrades\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tardigrades\\": "test/unit/"
    }
  },
  "require-dev": {
    "tardigrades/sexy-field": "dev-sym5",
    "doctrine/doctrine-bundle": "^2.2",
    "phpunit/phpunit": "^9",
    "mockery/mockery": "^1.0",
    "mikey179/vfsstream": "^1.6"
  }
}
